<?xml version="1.0" encoding="UTF-8"?><record_update table="sp_ng_template">
    <sp_ng_template action="INSERT_OR_UPDATE">
        <id>appt-queueTemplate</id>
        <sp_widget display_value="Online Appointment Scheduler Experience">f151899c0725101035cab0027cd30004</sp_widget>
        <sys_class_name>sp_ng_template</sys_class_name>
        <sys_created_by>admin</sys_created_by>
        <sys_created_on>2020-06-20 00:56:23</sys_created_on>
        <sys_id>69965c850761101035cab0027cd300cf</sys_id>
        <sys_mod_count>0</sys_mod_count>
        <sys_name>appt-queueTemplate</sys_name>
        <sys_package display_value="Appointment Scheduler" source="sn_appointment_sch">0334149407e1101035cab0027cd30098</sys_package>
        <sys_policy/>
        <sys_scope display_value="Appointment Scheduler">0334149407e1101035cab0027cd30098</sys_scope>
        <sys_update_name>sp_ng_template_69965c850761101035cab0027cd300cf</sys_update_name>
        <sys_updated_by>admin</sys_updated_by>
        <sys_updated_on>2020-06-20 00:56:23</sys_updated_on>
        <template><![CDATA[<div class="queue-container">  <h4 class="h4 padding-left">    {{data.msgs.queueStatus}}  </h4>  <section class="queue-section">    <div class="currently-served">      <h5 id="currently-serviced" class="h5 padding-left">        {{data.msgs.currentServiced}}      </h5>      <div role="list" aria-labelledby="currently-serviced">        <div ng-class="{'current-user': user.current}" class="user-item" ng-repeat="user in c.nowServing" role="listitem">          <span ng-if="user.name"class="username">{{user.name}}</span>          <span ng-if="!user.name"class="username"><em>{{data.msgs.userUnknown}}</em></span>        </div>      </div>    </div>    <div class="up-next">      <div class="queue-header">        <h5 id="up-next" class="h5 left-container padding-left">          {{data.msgs.upNext}}        </h5>        <h5 class="h5 right-container padding-right">          {{data.msgs.checkInTime}}        </h5>      </div>      <div ng-if="!c.waitingList.notEmpty">        <h5 class="semi-bold padding-left h4">          {{data.msgs.emptyQueueMsg}}        </h5>      </div>      <div role="list" aria-labelledby="up-next">        <div ng-if="c.waitingList.notEmpty" ng-init="count=1">          <div ng-class="{'current-user': user.current}" class="user-item" ng-repeat="user in c.waitingList.firstUpNext" role="listitem">            <div ng-class="{'left-container': !c.waitingList.waitOption.none}">              <span ng-if="!user.isAppointment" class="number">{{user.index}}.</span>              <span ng-if="user.name"class="username">{{user.name}}</span>              <span ng-if="!user.name"class="username"><em>{{data.msgs.userUnknown}}</em></span>            </div>            <div ng-if="!c.waitingList.waitOption.none && !user.isAppointment" class="time right-container">              {{user.timeWaited}}            </div>            <div ng-if="user.isAppointment" class="time right-container">              <span class="icon-calendar"></span>              {{user.apptmntTime}}            </div>          </div>          <hr ng-if="c.waitingList.renderDivider" />          <div ng-class="{'current-user': user.current}" class="user-item" ng-repeat="user in c.waitingList.secondUpNext" role="listitem">            <div ng-class="{'left-container': !c.waitingList.waitOption.none}">              <span class="number" ng-if="!user.isAppointment">{{user.index}}.</span>              <span ng-if="user.name"class="username">{{user.name}}</span>              <span ng-if="!user.name"class="username"><em>{{data.msgs.userUnknown}}</em></span>            </div>            <div ng-if="!c.waitingList.waitOption.none && !user.isAppointment" class="time right-container">              {{user.timeWaited}}            </div>            <div ng-if="user.isAppointment" class="time right-container">              <span class="icon-calendar"></span>              {{user.apptmntTime}}            </div>          </div>        </div>      </div>    </div>  </section> <!-- End of queue-section --></div>]]></template>
    </sp_ng_template>
</record_update>
